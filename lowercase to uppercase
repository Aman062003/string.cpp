// Online C++ compiler to run C++ program online
#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main() {
    string str = "awsdrftghnvb";
    // cout<< 'a' - 'A' <<endl;
    
    //convert lowercase to uppdercase
    for(int i =0; i<str.size(); i++)
    {
        if(str[i] >= 'a' && str[i] <='z')
        str[i] -= 32;
    }
    
    cout<<str<<endl;
    return 0;
}
